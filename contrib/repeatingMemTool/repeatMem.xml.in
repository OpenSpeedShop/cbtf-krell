<MRNet xmlns="http://www.krellinst.org/CBTF/MRNet.xsd">


    <Type>repeatMemNetwork</Type>
    <Version>1.0.0</Version>

    <Stream>
        <Name>DownStream</Name>
        <Tag>10000</Tag>
    </Stream>

    <Stream>
        <Name>UpStream</Name>
        <Tag>10001</Tag>
    </Stream>

    <Stream>
        <Name>TermStream</Name>
        <Tag>10002</Tag>
    </Stream>

    <Stream>
        <Name>NodeMemoryStream</Name>
        <Tag>10003</Tag>
    </Stream>

    <Stream>
        <Name>NodeMemoryVectStream</Name>
        <Tag>10004</Tag>
    </Stream>

    <Stream>
        <Name>StatStream</Name>
        <Tag>10005</Tag>
    </Stream>

    <Input>
        <Name>in</Name>
        <To><Input>system_in</Input></To>
    </Input>

    <Input>
        <Name>freq_in</Name>
        <To><Input>system_freq_in</Input></To>
    </Input>

    <Input>
        <Name>numBE_in</Name>
        <To><Input>system_numBE_in</Input></To>
    </Input>

    <Output>
        <Name>elapsed_time_out</Name>
        <From><Output>system_elapsed_time_out</Output></From>
    </Output>

    <Output>
        <Name>term_out</Name>
        <From><Output>system_term_out</Output></From>
    </Output>

    <Output>
        <Name>mem_out</Name>
        <From><Output>system_mem_out</Output></From>
    </Output>

    <Output>
        <Name>bin_out</Name>
        <From><Output>system_bin_out</Output></From>
    </Output>

    <Output>
        <Name>stat_out</Name>
        <From><Output>system_stat_out</Output></From>
    </Output>

    <Output>
        <Name>out</Name>
        <From><Output>system_out</Output></From>
    </Output>

    <Frontend>
        <Network>
            <Type>repeatMemNetwork_Frontend</Type>
            <Version>1.0.0</Version>

            <SearchPath>@@contriblib_location@@</SearchPath>
            <Plugin>mrnetPlugin.so</Plugin>
            <Plugin>repeatMemPlugin.so</Plugin>
            <Plugin>statisticsPlugin.so</Plugin>

            <Component>
                <Name>Sort</Name>
                <Type>AggregateMemory</Type>
            </Component>

            <Component>
                <Name>Bin</Name>
                <Type>BinMemory</Type>
            </Component>

            <Component>
                <Name>EndCircut</Name>
                <Type>EndCircutLogic</Type>
            </Component>

            <Component>
                <Name>StartCircut</Name>
                <Type>BeginCircutLogic</Type>
            </Component>

            <Component>
                <Name>AggStats</Name>
                <Type>KrellInstitute::CBTF::StatisticsAggregator&lt;long double&gt;</Type>
            </Component>

            <!--
            <Component>
                <Name>PacketToString</Name>
                <Type>ConvertPacketToString</Type>
            </Component>

            <Component>
                <Name>StringToPacket</Name>
                <Type>ConvertStringToPacket</Type>
            </Component>
            -->
            <Input>
                <Name>Frontend_In</Name>
                <To>
                    <Name>EndCircut</Name>
                    <Input>MemoryInfoIn</Input>
                </To>
            </Input>

            <Input>
                <Name>system_freq_in</Name>
                <To>
                    <Name>StartCircut</Name>
                    <Input>FrequencyIn</Input>
                </To>
            </Input>

            <Input>
                <Name>system_in</Name>
                <To>
                    <Name>StartCircut</Name>
                    <Input>FilenameIn</Input>
                </To>
            </Input>

            <Input>
                <Name>system_numBE_in</Name>
                <To>
                    <Name>EndCircut</Name>
                    <Input>NumBEIn</Input>
                </To>
            </Input>

            <Input>
                <Name>Term_In</Name>
                <To>
                    <Name>EndCircut</Name>
                    <Input>TermIn</Input>
                </To>
            </Input>

            <Input>
                <Name>NodeMemoryVect_In</Name>
                <To>
                    <Name>Sort</Name>
                    <Input>NodeMemoryVectorIn</Input>
                </To>
            </Input>

            <Input>
                <Name>Statistics_In</Name>
                <To>
                    <Name>AggStats</Name>
                    <Input>StatisticsIn</Input>
                </To>
            </Input>

            <Connection>
                <From>
                    <Name>EndCircut</Name>
                    <Output>RestartOut</Output>
                </From>
                <To>
                    <Name>StartCircut</Name>
                    <Input>RestartIn</Input>
                </To>
            </Connection>

            <Connection>
                <From>
                    <Name>EndCircut</Name>
                    <Output>TermOut</Output>
                </From>
                <To>
                    <Name>Sort</Name>
                    <Input>TermIn</Input>
                </To>
            </Connection>

            <Connection>
                <From>
                    <Name>Sort</Name>
                    <Output>NodeMemoryVectorOut</Output>
                </From>
                <To>
                    <Name>Bin</Name>
                    <Input>NodeMemoryVectorIn</Input>
                </To>
            </Connection>

            <Connection>
                <From>
                    <Name>Sort</Name>
                    <Output>TermOut</Output>
                </From>
                <To>
                    <Name>Bin</Name>
                    <Input>TermIn</Input>
                </To>
            </Connection>

            <Connection>
                <From>
                    <Name>StartCircut</Name>
                    <Output>StartTimeOut</Output>
                </From>
                <To>
                    <Name>EndCircut</Name>
                    <Input>StartTimeIn</Input>
                </To>
            </Connection>

            <Output>
                <Name>system_out</Name>
                <From>
                    <Name>EndCircut</Name>
                    <Output>MemoryInfoOut</Output>
                </From>
            </Output>

            <Output>
                <Name>system_elapsed_time_out</Name>
                <From>
                    <Name>EndCircut</Name>
                    <Output>ElapsedTimeOut</Output>
                </From>
            </Output>

            <Output>
                <Name>system_stat_out</Name>
                <From>
                    <Name>AggStats</Name>
                    <Output>StatisticsOut</Output>
                </From>
            </Output>

            <Output>
                <Name>system_bin_out</Name>
                <From>
                    <Name>Bin</Name>
                    <Output>BinsOut</Output>
                </From>
            </Output>

            <Output>
                <Name>system_mem_out</Name>
                <From>
                    <Name>Sort</Name>
                    <Output>NodeMemoryVectorOut</Output>
                </From>
            </Output>

            <Output>
                <Name>Frontend_Out</Name>
                <From>
                    <Name>StartCircut</Name>
                    <Output>FilenameOut</Output>
                </From>
            </Output>

            <Output>
                <Name>system_term_out</Name>
                <From>
                    <Name>Sort</Name>
                    <Output>TermOut</Output>
                </From>
            </Output>

        </Network>

        <IncomingUpstream>
            <Name>UpStream</Name>
            <To><Input>Frontend_In</Input></To>
        </IncomingUpstream>

        <IncomingUpstream>
            <Name>TermStream</Name>
            <To><Input>Term_In</Input></To>
        </IncomingUpstream>

        <IncomingUpstream>
            <Name>NodeMemoryVectStream</Name>
            <To><Input>NodeMemoryVect_In</Input></To>
        </IncomingUpstream>

        <IncomingUpstream>
            <Name>StatStream</Name>
            <To><Input>Statistics_In</Input></To>
        </IncomingUpstream>

        <OutgoingDownstream>
            <Name>DownStream</Name>
            <From><Output>Frontend_Out</Output></From>
        </OutgoingDownstream>

    </Frontend>

    <Filter>
        <Depth>
            <RootRelative>
                <Offset>1</Offset>
            </RootRelative>
        </Depth>
        <Network>
            <Type>repeatMemNetwork_Filter</Type>
            <Version>1.0.0</Version>
            <SearchPath>@@contriblib_location@@</SearchPath>
            <Plugin>repeatMemPlugin.so</Plugin>
            <Plugin>mrnetPlugin.so</Plugin>
            <Plugin>statisticsPlugin.so</Plugin>

            <Component>
                <Name>Sort</Name>
                <Type>AggregateMemory</Type>
            </Component>

            <Component>
                <Name>AggStats</Name>
                <Type>KrellInstitute::CBTF::StatisticsAggregator&lt;long double&gt;</Type>
            </Component>

            <Input>
                <Name>NodeMemoryVect_In</Name>
                <To>
                    <Name>Sort</Name>
                    <Input>NodeMemoryVectorIn</Input>
                </To>
            </Input>

            <Input>
                <Name>Statistics_In</Name>
                <To>
                    <Name>AggStats</Name>
                    <Input>StatisticsIn</Input>
                </To>
            </Input>

            <Input>
                <Name>Terminate_In</Name>
                <To>
                    <Name>Sort</Name>
                    <Input>TermIn</Input>
                </To>
            </Input>

            <Output>
                <Name>SortedVect_Out</Name>
                <From>
                    <Name>Sort</Name>
                    <Output>NodeMemoryVectorOut</Output>
                </From>
            </Output>

            <Output>
                <Name>Statistics_Out</Name>
                <From>
                    <Name>AggStats</Name>
                    <Output>StatisticsOut</Output>
                </From>
            </Output>

            <Output>
                <Name>Terminate_Out</Name>
                <From>
                    <Name>Sort</Name>
                    <Output>TermOut</Output>
                </From>
            </Output>
        </Network>

        <IncomingUpstream>
            <Name>StatStream</Name>
            <To><Input>Statistics_In</Input></To>
        </IncomingUpstream>

        <OutgoingUpstream>
            <Name>StatStream</Name>
            <From><Output>Statistics_Out</Output></From>
        </OutgoingUpstream>

        <IncomingUpstream>
            <Name>NodeMemoryVectStream</Name>
            <To><Input>NodeMemoryVect_In</Input></To>
        </IncomingUpstream>

        <OutgoingUpstream>
            <Name>NodeMemoryVectStream</Name>
            <From><Output>SortedVect_Out</Output></From>
        </OutgoingUpstream>

        <IncomingUpstream>
            <Name>TermStream</Name>
            <To><Input>Terminate_In</Input></To>
        </IncomingUpstream>

        <OutgoingUpstream>
            <Name>TermStream</Name>
            <From><Output>Terminate_Out</Output></From>
        </OutgoingUpstream>

    </Filter>

    <Backend>
        <Network>
            <Type>repeatMemNetwork_Backend</Type>
            <Version>1.0.0</Version>
            <SearchPath>@@contriblib_location@@</SearchPath>
            <Plugin>repeatMemPlugin.so</Plugin>
            <Plugin>mrnetPlugin.so</Plugin>
            <Plugin>statisticsPlugin.so</Plugin>

            <Component>
                <Name>getPID</Name>
                <Type>getPID</Type>
            </Component>

            <Component>
                <Name>getMem</Name>
                <Type>memPlugin</Type>
            </Component>

            <Component>
                <Name>makeVector</Name>
                <Type>AggregateMemory</Type>
            </Component>

            <Component>
                <Name>ValToStat</Name>
                <Type>KrellInstitute::CBTF::ValueToStatistics&lt;long double&gt;</Type>
            </Component>

            <Component>
                <Name>ValToPair</Name>
                <Type>KrellInstitute::CBTF::CreateValIdPair&lt;long double&gt;</Type>
            </Component>

            <Input>
                <Name>Backend_In</Name>
                <To>
                    <Name>getPID</Name>
                    <Input>in</Input>
                </To>
            </Input>

            <Output>
                <Name>Backend_Output</Name>
                <From>
                    <Name>getMem</Name>
                    <Output>out</Output>
                </From>
            </Output>

            <Output>
                <Name>Term_Signal</Name>
                <From>
                    <Name>getPID</Name>
                    <Output>TermOut</Output>
                </From>
            </Output>

            <Output>
                <Name>NodeMemoryVect_Out</Name>
                <From>
                    <Name>makeVector</Name>
                    <Output>NodeMemoryVectorOut</Output>
                </From>
            </Output>

            <Output>
                <Name>Statistics_Out</Name>
                <From>
                    <Name>ValToStat</Name>
                    <Output>StatisticsOut</Output>
                </From>
            </Output>

            <Connection>
                <From>
                    <Name>getPID</Name>
                    <Output>out</Output>
                </From>
                <To>
                    <Name>getMem</Name>
                    <Input>in</Input>
                </To>
            </Connection>

            <Connection>
                <From>
                    <Name>getMem</Name>
                    <Output>NodeMemoryOut</Output>
                </From>
                <To>
                    <Name>makeVector</Name>
                    <Input>NodeMemoryIn</Input>
                </To>
            </Connection>

            <Connection>
                <From>
                    <Name>getMem</Name>
                    <Output>ApplicationOut</Output>
                </From>
                <To>
                    <Name>ValToPair</Name>
                    <Input>ValueIn</Input>
                </To>
            </Connection>

            <Connection>
                <From>
                    <Name>getMem</Name>
                    <Output>IdOut</Output>
                </From>
                <To>
                    <Name>ValToPair</Name>
                    <Input>IdIn</Input>
                </To>
            </Connection>

            <Connection>
                <From>
                    <Name>ValToPair</Name>
                    <Output>PairOut</Output>
                </From>
                <To>
                    <Name>ValToStat</Name>
                    <Input>PairIn</Input>
                </To>
            </Connection>

        </Network>

        <IncomingDownstream> 
            <Name>DownStream</Name>
            <To><Input>Backend_In</Input></To>
        </IncomingDownstream>

        <OutgoingUpstream>
            <Name>NodeMemoryVectStream</Name>
            <From><Output>NodeMemoryVect_Out</Output></From>
        </OutgoingUpstream>

        <OutgoingUpstream>
            <Name>UpStream</Name>
            <From><Output>Backend_Output</Output></From>
        </OutgoingUpstream>

        <OutgoingUpstream>
            <Name>TermStream</Name>
            <From><Output>Term_Signal</Output></From>
        </OutgoingUpstream>

        <OutgoingUpstream>
            <Name>StatStream</Name>
            <From><Output>Statistics_Out</Output></From>
        </OutgoingUpstream>

    </Backend>

</MRNet>
